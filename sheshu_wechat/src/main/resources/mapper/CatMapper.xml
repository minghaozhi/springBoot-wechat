<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.sheshu.heat.mapper.CatMapper">

    <insert id="insert">
        INSERT INTO `cat`
        (`name`,`age`)
        VALUES (#{name},#{age})
    </insert>

</mapper>